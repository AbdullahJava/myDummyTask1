    <changeSet id="5" author="asd">
        <createTable tableName="employee">
            <column autoIncrement="true"
                    name="id"
                    type="INT">
                <constraints nullable="false"
                             primaryKey="true"/>
            </column>
            <column name="name"
                    type="VARCHAR(255)">
            </column>
            <column name="editor"
                    type="VARCHAR(255)">
            </column>
            <column name="phone"
                    type="MEDIUMTEXT"/>
            <column name="address"
                    type="VARCHAR(255)"/>
            <column name="is_active"
                    type="	TINYINT"/>
        </createTable>
    </changeSet>
</databaseChangeLog>